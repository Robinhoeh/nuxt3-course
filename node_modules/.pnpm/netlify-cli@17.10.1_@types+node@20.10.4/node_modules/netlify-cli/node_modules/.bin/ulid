#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../ulid@2.3.0/node_modules/ulid/bin/cli.js" "$@"
else
  exec node  "$basedir/../../../../../ulid@2.3.0/node_modules/ulid/bin/cli.js" "$@"
fi
