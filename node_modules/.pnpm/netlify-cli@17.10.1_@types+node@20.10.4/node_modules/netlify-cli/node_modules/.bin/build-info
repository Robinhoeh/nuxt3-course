#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../@netlify+build-info@7.11.3/node_modules/@netlify/build-info/bin.js" "$@"
else
  exec node  "$basedir/../../../../../@netlify+build-info@7.11.3/node_modules/@netlify/build-info/bin.js" "$@"
fi
