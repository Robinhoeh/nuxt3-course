#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../lambda-local@2.1.2/node_modules/lambda-local/build/cli.js" "$@"
else
  exec node  "$basedir/../../../../../lambda-local@2.1.2/node_modules/lambda-local/build/cli.js" "$@"
fi
