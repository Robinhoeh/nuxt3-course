#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../ipx@2.0.1_@netlify+blobs@6.3.1/node_modules/ipx/bin/ipx.mjs" "$@"
else
  exec node  "$basedir/../../../../../ipx@2.0.1_@netlify+blobs@6.3.1/node_modules/ipx/bin/ipx.mjs" "$@"
fi
