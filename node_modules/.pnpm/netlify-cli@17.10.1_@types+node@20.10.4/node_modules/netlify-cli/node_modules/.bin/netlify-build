#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../@netlify+build@29.31.0_@types+node@20.10.4_debug@4.3.4/node_modules/@netlify/build/bin.js" "$@"
else
  exec node  "$basedir/../../../../../@netlify+build@29.31.0_@types+node@20.10.4_debug@4.3.4/node_modules/@netlify/build/bin.js" "$@"
fi
