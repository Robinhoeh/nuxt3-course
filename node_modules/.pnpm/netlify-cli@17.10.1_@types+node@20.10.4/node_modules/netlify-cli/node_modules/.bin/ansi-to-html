#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../ansi-to-html@0.7.2/node_modules/ansi-to-html/bin/ansi-to-html" "$@"
else
  exec node  "$basedir/../../../../../ansi-to-html@0.7.2/node_modules/ansi-to-html/bin/ansi-to-html" "$@"
fi
