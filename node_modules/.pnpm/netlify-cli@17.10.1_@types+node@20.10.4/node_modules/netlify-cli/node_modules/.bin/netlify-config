#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../@netlify+config@20.10.0/node_modules/@netlify/config/bin.js" "$@"
else
  exec node  "$basedir/../../../../../@netlify+config@20.10.0/node_modules/@netlify/config/bin.js" "$@"
fi
