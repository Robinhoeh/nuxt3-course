#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../prettyjson@1.2.5/node_modules/prettyjson/bin/prettyjson" "$@"
else
  exec node  "$basedir/../../../../../prettyjson@1.2.5/node_modules/prettyjson/bin/prettyjson" "$@"
fi
